{
  "$id": "ce_helpArticle",
  "$schema": "https://schema.yext.com/config/km/field-presentation/v1",
  "entityType": "yext/helpArticle",
  "displayName": "Help Article",
  "presentationEntries": [
    {
      "displayName": "Content",
      "entryType": "DIVIDER"
    },
    {
      "displayName": "Core Information",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/name",
          "visibleOnAdd": true
        },
        {
          "id": "yext/entityId",
          "visibleOnAdd": true
        }
      ]
    },
    {
      "displayName": "Article Details",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/body",
          "visibleOnAdd": true
        },
        {
          "id": "freshdesk_categoryID"
        },
        {
          "id": "freshdesk_folderID"
        },
        {
          "id": "freshdesk_dateCreated"
        },
        {
          "id": "freshdesk_dateUpdated"
        },
        {
          "id": "freshdesk_tags"
        },
        {
          "id": "freshdesk_views"
        },
        {
          "id": "freshdesk_thumbsUp"
        },
        {
          "id": "freshdesk_thumbsDown"
        }
      ]
    },
    {
      "displayName": "Additional Attributes",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/shortDescription"
        },
        {
          "id": "yext/keywords"
        },
        {
          "id": "yext/landingPageUrl"
        },
        {
          "id": "yext/promoted"
        },
        {
          "id": "yext/voteCount"
        },
        {
          "id": "yext/voteSum"
        }
      ]
    },
    {
      "displayName": "Settings",
      "entryType": "DIVIDER"
    },
    {
      "defaultCollapsed": true,
      "displayName": "Internal Use Only",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/folder"
        },
        {
          "id": "yext/labels"
        },
        {
          "id": "yext/timezone"
        }
      ]
    },
    {
      "defaultCollapsed": true,
      "displayName": "Knowledge Assistant",
      "entryType": "SECTION",
      "fields": [
        {
          "id": "yext/primaryConversationContact"
        },
        {
          "id": "yext/nudgeEnabled"
        }
      ]
    }
  ]
}